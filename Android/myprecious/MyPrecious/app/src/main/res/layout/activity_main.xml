<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.project.myprecious.elvis.myprecious.MainActivity">

    <Button
        android:id="@+id/goSangmin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="goSangmin" />

    <Button
        android:id="@+id/goGoohyun"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/goSangmin"
        android:text="goGoohyun" />

    <EditText
        android:layout_width="220dp"
        android:layout_height="wrap_content"
        android:id="@+id/phoneText"
        android:inputType="phone"
        android:ems="10"
        android:hint="전화번호 입력"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="93dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="입장"
        android:id="@+id/enterBtn"
        android:layout_alignBaseline="@+id/phoneText"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"/>

</RelativeLayout>
